<script>
	function cookieConsent() {
 return {
 display: true,
 margin: '-mb-64',

 setCookie() {
 let expiresDate = new Date();
 expiresDate.setDate(expiresDate.getDate() + 1).toString();
 document.cookie = "cookieconsent=1; path=/; expires=" + expiresDate;
 this.margin = '-mb-64';
 },

 checkCookie() {
 let cookies = document.cookie;
 cookies = cookies.split("; ").map(cookie => {
 return cookie.split("=");
 });

 const cookieConsent = cookies.find(cookie => cookie[0] === 'cookieconsent');

 if (cookieConsent === undefined || cookieConsent[1] !== '1') {
 this.display = true;
 this.margin = 'mb:auto';
 }
 }
 		}
	}
