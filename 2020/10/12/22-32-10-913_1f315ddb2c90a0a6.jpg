<script>
 function coinTableData() {
 return {
 data: null,
 isLoading: true,
 hubpage: false,
 currency: 'USD',
 locale: 'en',
 country: 'US',
 limit: '10',
 showCta: '',
 ctaText: 'Buy/Sell',
 cols: ["PRICE","24H"],
 offers: false,
 defaultOffer: '',
 pages: JSON.stringify( {"posts":[{"permalink":"https:\/\/coinjournal.net\/neo\/price\/","symbol":"NEO"},{"permalink":"https:\/\/coinjournal.net\/zcash\/price\/","symbol":"ZEC"},{"permalink":"https:\/\/coinjournal.net\/tezos\/price\/","symbol":"XTZ"},{"permalink":"https:\/\/coinjournal.net\/monero\/price\/","symbol":"XMR"},{"permalink":"https:\/\/coinjournal.net\/bitcoin-cash\/price\/","symbol":"BCH"},{"permalink":"https:\/\/coinjournal.net\/ethereum\/price\/","symbol":"ETH"},{"permalink":"https:\/\/coinjournal.net\/litecoin\/price\/","symbol":"LTC"},{"permalink":"https:\/\/coinjournal.net\/bitcoin\/price\/","symbol":"BTC"},{"permalink":"https:\/\/coinjournal.net\/eos\/price\/","symbol":"EOS"},{"permalink":"https:\/\/coinjournal.net\/ripple\/price\/","symbol":"XRP"},{"permalink":"https:\/\/coinjournal.net\/ethereum-classic\/price\/","symbol":"ETC"},{"permalink":"https:\/\/coinjournal.net\/dash\/price\/","symbol":"DASH"},{"permalink":"https:\/\/coinjournal.net\/cardano\/price\/","symbol":"ADA"},{"permalink":"https:\/\/coinjournal.net\/stellar-lumens\/price\/","symbol":"XLM"},{"permalink":"https:\/\/coinjournal.net\/tron\/price\/","symbol":"TRX"},{"permalink":"https:\/\/coinjournal.net\/binance-coin\/price\/","symbol":"BNB"},{"permalink":"https:\/\/coinjournal.net\/iota\/price\/","symbol":"MIOTA"},{"permalink":"https:\/\/coinjournal.net\/bitcoin-gold\/price\/","symbol":"BTG"},{"permalink":"https:\/\/coinjournal.net\/chainlink\/price\/","symbol":"LINK"}]}, ),
 fetchData() {
 this.isLoading = true;
 fetch(`https://pts.igms.io/api/v2/coins/latest?limit=${this.limit}`)
 .then((res) => res.json())
 .then((data) => {
 this.data = data.data;
 this.isLoading = false;
 });
 },
 formatPrice(value) {
 return new Intl.NumberFormat(this.locale + '-' + this.locale.toUpperCase(), {
 style: 'currency',
 currency: this.currency
 }).format(value);
 },
 formatNumber(value) {
 return new Intl.NumberFormat(this.locale + '-' + this.locale.toUpperCase()).format(value);
 },
 isPositive(value) {
 return Math.sign(value) === 1;
 },
 getPropertyForQuote(quote, property) {
 const quoteByCurrency = quote[this.currency];
 return quoteByCurrency[property];
 },
 prefixAmount(value) {
 if (Math.abs(Number(value)) >= 1.0e+9) {
 return (Math.abs(Number(value)) / 1.0e+9).toFixed(2) + " Billion";
 } else if (Math.abs(Number(value)) >= 1.0e+6) {
 return (Math.abs(Number(value)) / 1.0e+6).toFixed(2) + " Million";
 } else if (Math.abs(Number(value)) >= 1.0e+3) {
 return (Math.abs(Number(value)) / 1.0e+3).toFixed(2) + " Thousand"
 } else {
 return Math.abs(Number(value)).toFixed(2);
 }
 },
 getOfferUrl(symbol, position) {
 const coin = symbol.toLowerCase();
 const itemPosition = position + 1;
 const element = document.getElementById(symbol);
 const query = `?component=price-table&language=${this.locale}&country=${this.country}&position=${itemPosition}&totalPositions=${this.limit}&channel=crypto&coin=${coin}`;

 if (this.offers && this.offers.hasOwnProperty(coin)) {
 const data = audienceTracking.generateDataFromVisitLink(this.offers[coin] + query);
 const partnerData = data.partnerData;
 if (element) {
 element.dataset.partnerData = JSON.stringify(partnerData);
 }
 return data.url;
 } else if (this.defaultOffer) {
 const data = audienceTracking.generateDataFromVisitLink(this.defaultOffer + query);
 const partnerData = data.partnerData;
 if (element) {
 element.dataset.partnerData = JSON.stringify(partnerData);
 }
 return data.url;
 } else {
 return false;
 }
 },
 getPermalink(coin) {
 const pages = JSON.parse(this.pages);
 if (pages != '') {
 for(var i = 0; i < pages.posts.length; i++) {
 if (pages.posts[i].symbol == coin) {
 return pages.posts[i].permalink;
 }
 }
 }
 },
 displayCol(value) {
 if (!this.cols) {
 return true;
 } else if (this.cols && this.cols.includes(value)) {
 return true;
 } else {
 return false;
 }
 }
 };
 }
</script>

